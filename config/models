User
    ident Text
    token Text
    UniqueUser ident
    deriving Typeable
Repository
    githubId String
    name Text
    ownerLogin Text
    UniqueRepository githubId
    deriving Show
Issue
    githubId String
    number String
    repositoryId RepositoryId
    UniqueIssue githubId
    deriving Show
Estimate
    issueId IssueId
    userId UserId
    points Int
    UniqueEstimate issueId userId
    deriving Show

